{
  "name": "app-config",
  "description": "12-factor, layered configuration library for Node apps",
  "version": "0.0.1",
  "type": "module",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "app-config": "./bin/app-config"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "bootstrap": "pnpm run build",
    "check-exports": "attw --pack .",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc"
  },
  "peerDependencies": {
    "zod": "^3.24.2"
  },
  "dependencies": {
    "chokidar": "4.0.3",
    "esbuild": "0.25.0",
    "json5": "2.2.3",
    "meow": "13.2.0",
    "yaml": "2.7.0",
    "zod-to-json-schema": "3.24.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "catalog:",
    "@types/lodash-es": "4.17.12",
    "@vitest/coverage-v8": "3.0.6",
    "eslint": "catalog:",
    "lodash-es": "4.17.21",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "zod": "3.24.2"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
